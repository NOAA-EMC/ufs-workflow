%include "%ECF_INCLUDE%/head.h"

module use /work/noaa/marine/Jian.Kuang/ufs-weather-model/modulefiles
module load ufs_orion.intel
module list

ulimit -s unlimited

echo "Model started:  " `date`

export OMP_STACKSIZE=512M
export KMP_AFFINITY=scatter
export OMP_NUM_THREADS=1

eval $(ewok_source_config $CONFIG_FILE current_dir stage_dir)
cd $current_dir
pwd

mkdir RESTART
cp /work/noaa/marine/Jian.Kuang/ufs-weather-model/build-fv3/ufs_model .
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/INPUT/fv_core.res.nc INPUT/fv_core.res.nc
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/global_* .
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/co2* .
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/params_grib2_tbl_new .
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/fd_nems.yaml .
cp /work/noaa/stmp/jkuang/stmp/jkuang/FV3_RT/rt_227267/control_restart/postxconfig-NT* .

srun ufs_model

%include "%ECF_INCLUDE%/tail.h"
